{
  "name": "band-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test:unit",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^3.2.1",
    "@tsed/ajv": "^6.75.1",
    "@tsed/common": "^6.75.1",
    "@tsed/core": "^6.75.1",
    "@tsed/di": "^6.75.1",
    "@tsed/exceptions": "^6.75.1",
    "@tsed/graphql": "^6.75.1",
    "@tsed/json-mapper": "^6.75.1",
    "@tsed/passport": "^6.75.1",
    "@tsed/platform-express": "^6.75.1",
    "@tsed/schema": "^6.75.1",
    "ajv": "^8.6.3",
    "apollo-datasource": "^3.2.0",
    "apollo-datasource-rest": "^3.3.0",
    "apollo-server-express": "^3.4.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.6.1",
    "method-override": "^3.0.0",
    "passport": "^0.5.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.13.3",
    "@tsed/cli-plugin-jest": "3.13.3",
    "@tsed/cli-plugin-passport": "3.13.3",
    "@tsed/cli-plugin-prisma": "3.13.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.1",
    "@types/passport": "^1.0.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "apollo-server-testing": "^2.25.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default",
    "architecture": "default"
  }
}
